<div class="container">
    <div class="card">
        <div class="card-header">
            <h1>Lista de Clientes</h1>
            {{!-- <button class="btn btn-primary" onclick="document.location='/'">Voltar</button> --}}
            <br>
        </div>
        
    <table class="table table-striped" id="tableID">
        <tr>
             <th>ID</th>
            <th>Nome</th>    
          
        </tr>
        
        {{# each cliente }}
            <tr>
                <td>{{inc @index}}</td>
                <td>{{ this.nome }}</td>
            </tr>
        {{/ each }}
    </table>
    </div>    
</div>
